{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start Next.js dev server",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"isBackground": false,
			"problemMatcher": [
				"$tsc",
				"$eslint - stylish"
			],
			"group": "build"
		},
		{
			"label": "Install CDP SDK",
			"type": "shell",
			"command": "npm",
			"args": [
				"install",
				"@coinbase/cdp-sdk@latest"
			],
			"isBackground": false,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "Install Stripe SDK",
			"type": "shell",
			"command": "npm",
			"args": [
				"install",
				"stripe@^16.5.0"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}